/**index.wxss**/

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.info {
  padding: 10px 15px;
  font-size: 12px;
  color: #666666;
}
.footer {
  display: flex;
  flex-direction: row;
}
.footer .last {
  margin-left: 10px;
}
.footer label {
  font-size: 11px;
  color: #999999;
  line-height: 30px;
}
.footer .first {
  margin-left: auto;
}
.create-content {
  text-align: center;
  font-size: 14px;
}
.create-content view {
  margin-top: 20px;
}
.create-tip {
  color: #999999;
}
.create {
  color: #f65d72;
  text-decoration:underline;
}